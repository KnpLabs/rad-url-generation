{
    "name": "knplabs/rad-url-generation",
    "description": "Simply auto-complete needed route parameters with existing ones.",
    "license": "MIT",
    "keywords": ["routing", "router", "URL", "URI", "autocompletion", "rad"],
    "authors": [
        {
            "name": "KNPLabs Team",
            "email": "hello@knplabs.com"
        }
    ],
    "require": {
        "php": "~7.0",
        "symfony/config": "~2.8||~3.0",
        "symfony/dependency-injection": "~2.8||~3.0",
        "symfony/http-kernel": "~2.8||~3.0",
        "symfony/routing": "~2.8||~3.0"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.4",
        "knplabs/phpspec-welldone-extension": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Knp\\Rad\\UrlGeneration\\": "src/Knp/Rad/UrlGeneration/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
